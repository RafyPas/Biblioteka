<Window x:Class="Biblioteka.ReturnWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Biblioteka"
        mc:Ignorable="d"
        Title="Oddaj" Height="450" Width="800">
    <Grid x:Name="Background">
        <Grid x:Name="Content" Height="300" Width="500" HorizontalAlignment="Center" VerticalAlignment="Center" Background="DarkKhaki" Opacity="0.8">
            <Label Content="Zaloguj się:" Margin="175,40,175,210" Width="150" Height="40" FontSize="20" HorizontalContentAlignment="Center"></Label>
            <TextBox Margin="0,0,0,0" Width="250" Height="25" VerticalContentAlignment="Center" Text="Username"/>
            <TextBox Margin="0,80,0,0" Width="250" Height="25" VerticalContentAlignment="Center" Text="Password"/>
        </Grid>
    </Grid>
</Window>
